{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "import/order": ["error", {
      "alphabetize": {
        "order": "asc"
      },
      "newlines-between": "never"
    }],
    "prettier/prettier": "error",
    "tailwindcss/no-custom-classname": [
      "warn",
      {
        "whitelist": ["[A-Z].*"]
      }
    ]
  }
}
